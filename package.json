{
  "name": "cuppa",
  "version": "1.0.0",
  "description": "a MERN stack tea collection application",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "webpack-dev-server --config ./webpack.config.js --mode development",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "watch": "jest --watch",
    "coverage": "jest --coverage --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jenniferlynparsons/cuppa.git"
  },
  "author": "Jennifer Lyn Parsons",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jenniferlynparsons/cuppa/issues"
  },
  "homepage": "https://github.com/jenniferlynparsons/cuppa#readme",
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "axios": "^0.18.0",
    "babel-plugin-transform-typescript": "^7.0.0-alpha.19",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bulma": "^0.7.4",
    "bulma-pageloader": "^2.2.0",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "css-loader": "^2.1.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "file-loader": "^4.3.0",
    "http-proxy-middleware": "^0.19.2",
    "immutable": "^4.0.0-rc.12",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.10.11",
    "node-sass": "^7.0.0",
    "npm": "^6.14.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-api-middleware": "^3.2.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uuid": "^3.4.0",
    "validator": "^10.11.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@testing-library/react": "^8.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-jsx-remove-data-test-id": "^2.1.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.5.0",
    "jsdoc": "^3.6.6",
    "lodash": "^4.17.20",
    "moxios": "^0.4.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "^16.14.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.28.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "client/src/**/*.{js,jsx}",
      "!client/src/*.{js,jsx}",
      "!client/src/test/**",
      "!client/src/lib/**",
      "!client/src/components/App/**",
      "!client/src/components/Auth/PrivateRoute/**",
      "!client/src/components/ErrorBoundary/**",
      "!**/node_modules/**"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "setupFiles": [],
    "setupFilesAfterEnv": [
      "@testing-library/react/cleanup-after-each"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/__tests__/**/*.js?(x)"
    ],
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/src/test/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass|styl)$": "identity-obj-proxy"
    }
  }
}
